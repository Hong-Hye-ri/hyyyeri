function openCashbackEvent(cardwcd, position) {
    var winHieght = $(window).height();
    var winWidth = $(window).width();
    
    var winX = window.screenLeft;
    var winY = window.screenTop;
    
    var eventPopWidth = 890;
    var eventPopHeight = 820;
    
    var LeftPosition = (position? position.left : winX + (winWidth - eventPopWidth)/2);
    var TopPosition = (position? position.top : winY + (winHieght - eventPopHeight)/2);
    
    var eventPage = "cashback_pc";
    if (cardwcd) {
        var _cardwcd = "/" + cardwcd + "/";
        if ("/MBT/MPBT/M3PBT/".indexOf(_cardwcd) >= 0
                || "/XE3/XPE3/X3PE3/".indexOf(_cardwcd) >= 0) eventPage = "cashback_event";
        else if ("/ONOF/".indexOf(_cardwcd) >= 0) eventPage = "cashback_event_zero_all";
        else if ("/ZROE2/MZROE2/MOZRE2/MMOZRE2/".indexOf(_cardwcd) >= 0) eventPage = "cashback_zero";
        else if ("/MKTE3/MKTE3P/MKTE3TP/MLGHE3/MCJHE2D/MTBE2D/MKTRE3/MSKTRE3/MHDSHE3/".indexOf(_cardwcd) >= 0) eventPage = "cashback_alliance";
        else if ("/MLGTE3/MSKTE3/MCJE2/MCJFLYE2/MCCKE3/MCHHE3/MHDQE3/MLGEE3/MLGTRE3/".indexOf(_cardwcd) >= 0) eventPage = "cashback_light_v2";
        else if ("/MHMMOE3T/".indexOf(_cardwcd) >= 0) eventPage = "cashback_light_v2";
        else if ("/SLHC/".indexOf(_cardwcd) >= 0) eventPage = "cashback_event_ONOF";
        else if ("/MSPSE3/YSHC/HDSHHC/".indexOf(_cardwcd) >= 0) eventPage = "cashback_MSPSE3";
    }
    window.open("/inc/" + eventPage + ".html?act=click", "eventCashbackPopWin","width="+eventPopWidth+"=, height="+eventPopHeight+", top="+TopPosition +",left="+LeftPosition);
}

function openCashbackEventNew(cardwcd, newComer) {
    var winHieght = $(window).height();
    var winWidth = $(window).width();
    
    var winX = window.screenLeft;
    var winY = window.screenTop;
    
    var eventPopWidth = 890;
    var eventPopHeight = 820;
    
    var LeftPosition = winX + (winWidth - eventPopWidth)/2;
    var TopPosition = winY + (winHieght - eventPopHeight)/2;
    
    var eventPage = "cashback_pc";
    var _cardwcd = "/" + cardwcd + "/";
    if (newComer) {
        if ("/MBT/MPBT/M3PBT/".indexOf(_cardwcd) >= 0) eventPage = "cashback_event_m_all";
        else if ("/XE3/XPE3/X3PE3/".indexOf(_cardwcd) >= 0) eventPage = "cashback_event_x_all";
        else if ("/ZROE2/MZROE2/MOZRE2/MMOZRE2/".indexOf(_cardwcd) >= 0) eventPage = "cashback_event_zero_all";
        else if ("/SLHC/".indexOf(_cardwcd) >= 0) eventPage = "cashback_event_ONOF";
    } else {
        if ("/MBT/MPBT/M3PBT/".indexOf(_cardwcd) >= 0) eventPage = "cashback_event_2";
        else if ("/XE3/XPE3/X3PE3/".indexOf(_cardwcd) >= 0) eventPage = "cashback_event_2";
        else if ("/ZROE2/MZROE2/MOZRE2/MMOZRE2/".indexOf(_cardwcd) >= 0) eventPage = "cashback_zero_2";
        else if ("/SLHC/".indexOf(_cardwcd) >= 0) eventPage = "cashback_event_ONOF_2";
    }
    window.open("/inc/" + eventPage + ".html?act=click", "eventCashbackPopWin","width="+eventPopWidth+"=, height="+eventPopHeight+", top="+TopPosition +",left="+LeftPosition);
}
</script>
