        var changeMethod = function(href, tab){
            var div = $(href);
            var cls = div.attr("class").split(' ')[0];
            var tabs = tab.closest(".tab");
            var idxt = $(this).closest("li").index() + 1;

            tabs.find("li").removeClass("active");
            tab.closest("li").addClass("active");
            
            div.siblings("." + cls).hide();
            div.show();
			
			clearInterval(tid);
			clearInterval(qrcheckid);
			
			if($('#loginTab1').is(':visible')){ // 핀로그인
				//console.log("PINsign.isSupport() -> "+PINsign.isSupport());
				if (!PINsign.isSupport()) { //지원하지 않는 브라우저
					$("#id_pin_area_not").show();      //pin 미지원
					$("#id_pin_area_yet").hide();      //pin 미설정  시
					//$("#id_pin_area_bottom_y").hide(); //pin 미설정  시 하단버튼
					$("#id_pin_area_input").hide();    //pin 로그인
