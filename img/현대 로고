    <script type="text/javascript" charset="UTF-8">
    var isLoginStr = "false";
    var devEnv     = "P";
    var cookieSearchMenuWord = "_HC_MENU_WORD";
    var DIVE_RES_OBJ_KEY = 'diveResObjKey';
    var diveHomepageUrl = "https://dive.hyundaicard.com";
    
    $("#header").load("/docfiles/resources/pc/html/com/gnbList.html",function(){
        
        $("#devEnv").hide();
        if(isLoginStr == 'true'){
            $(".loginFreqNo").hide();
            $("#myInfoMng").show();
            //자주찾는 메뉴
            getMenuCookie();
            if(devEnv == "L" || devEnv == "D"){
                $("#devEnv").show();
            }
        }
        else{
            $(".loginFreqNo").show();//로그인이전
            $("#myInfoMng").hide();
        }
        
        //컬처 GNB > 컬처
        var diveMenuString = window.sessionStorage.getItem(DIVE_RES_OBJ_KEY);
        if (diveMenuString) {
              var diveMenuInfo = JSON.parse(diveMenuString);
              diveResView(diveMenuInfo);
              //console.log('diveMenu get session');
        }else{
            $.getJSON("https://dive.hyundaicard.com/api/extern/spaceGnbInfo.hdc"  
                    , ""
                    , function(res) {
                        //console.log(res);
                        var diveResObj = res;
                        diveResView(diveResObj);
                        window.sessionStorage.setItem(DIVE_RES_OBJ_KEY, JSON.stringify(diveResObj));
                    });  
            //console.log('diveMenu get dive');
        }
        
        //검색 레이어 열기
        $('.btn_search_open').on('click', function(){
            //$('.section-search').addClass('on').find('input[type=text]').focus();
            $('html').addClass('layer_active');
            $('.section-search').addClass('on');
            
            //검색어 삭제
            $('#topSearch').val('');
            //검색어 인풋에 포커스
            $('#topSearch').focus();
            
            //자동완성리스트 초기화
            $('.keyword_list').empty();
            
            //인기검색어
            showSearchPopularWord();
            
            //최근검색어
            showSearchRecentWord();     

        });
    
        //띠배너
        $("#topBannerWrapCms").load("/docfiles/cms/home/PCHOM000100_04.html",function(){commonUi.gnb.gnbHeader();});

    });

    //타이틀 변경
    function setDocTitle(doctitle){
        doctitle += '-현대카드';
        $(document).attr("title",doctitle);
        $("meta[name='title']").attr("content",doctitle);   
    }
    
