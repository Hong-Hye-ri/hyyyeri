        function doResetPinPass(){
            if(typeof inputPinPassPad != 'undefined'){
                inputPinPassPad.hidePINPad();
                inputPinPassPad.detach();
            }
        }
        
        
        function pinsignListInit(){
            if (!PINsignConfig.USE_OPEN_PINSIGN_SERVICE) {
                if(PINsign.ConnAdapter.getCurrentStatus()) {
                    setTimeout(function(){setPINsign_list();}, 1000);
                } else {
                    setPINsign_list();
                }
            }
        }
        
        function setPINsign_list() {
            window._time_start_list = new Date(); //debug
            if (!PINsign.isSupport()) {
                //미지원 브라우저
                //openAlertType1("간편번호 인증서가 지원하지 않는 브라우저입니다.");
                $("#id_pin_area_not").show();      //pin 미지원
                $("#id_pin_area_yet").hide();      //pin 미설정  시
                //$("#id_pin_area_bottom_y").hide(); //pin 미설정  시 하단버튼
                $("#id_pin_area_input").hide();    //pin 로그인
                $("#id_pin_area_input_bottom").hide();    //pin 재설정
                //$("#id_pin_area_bottom_cu").hide();//pin 로그인 하단버튼
                return;
            }            
            PINsign.list({}, completePINsign_list);
        }
        
        
        function completePINsign_list(result) {
            window._result_pinsign_list = result; //debug
            if (window._time_start_list) window._time_exec_list = Math.floor((new Date()).getTime()-window._time_start_list.getTime())/1000; //debug
            if(result.error) {
                var errCode = result.error.code;
                var errMsg = SITE_getPINsignUserMsg("list", result.error);
                //openAlertType1(errMsg);
                $("#id_pinErr").show();
                $("#id_pinErr").html(errMsg);
                $("#inputPinPass").addClass('error');
                $("#inputPinPassGud").hide();
                $("#inputPinPassBg").addClass('error');
            } else if(result.data.length == 0) {
                //브라우저에 저장된 베리가 없음
				$("#id_pin_area_not").hide();      //pin 미지원
				$("#id_pin_area_yet").show();      //pin 미설정  시
				//$("#id_pin_area_bottom_y").show(); //pin 미설정  시 하단버튼
				$("#id_pin_area_input").hide();    //pin 로그인
				$("#id_pin_area_input_bottom").hide();    //pin 재설정
				//$("#id_pin_area_bottom_cu").hide();//pin 로그인 하단버튼
            } else {
			    //브라우저에 저장된 베리 있음
				$("#id_pin_area_not").hide();      //pin 미지원
				$("#id_pin_area_yet").hide();      //pin 미설정  시
				//$("#id_pin_area_bottom_y").hide(); //pin 미설정  시 하단버튼
				$("#id_pin_area_input").show();    //pin 로그인
				$("#id_pin_area_input_bottom").show();    //pin 재설정
				//$("#id_pin_area_bottom_cu").hide();//pin 로그인 하단버튼
				
                $("#id2key").val(result.data[0].id);
            }
        }
        
        //PIN패드 초기화
        var _defaultLoginPINpadVisible = false; //PIN패드 초기화시 키패드 디스플레이 여부
        function setLoginPinpadInit(targetId) {
            
            //오류메시지 지우기
            var formPinLoginDiv = document.getElementById('formPinLogin'); 
            formPinLoginDiv.addEventListener('click', function(event){
                $("#id_pinErr").html('');
                $("#id_pinErr").hide();
                $("#inputPinPass").removeClass('error');
                $("#inputPinPassGud").show();
                $("#inputPinPassBg").removeClass('error');
                //console.log('formPinLoginDiv click');
            }, false);
            
            var pinpadUIType = (PINsign.Util.isMobile()) ? "mobile" : "mobile";
            window.inputPinPassPad = new PINsignUI.PasswordInput({pinpadUIType:pinpadUIType,pinpadContainerSel:"#mobile-pinpad"});

            var completePINsign_pinpad = function(securePIN) {
                if(securePIN.getLength() != 6) {
                    //openAlertType1("6자리 간편번호를 입력해주세요.");
                    $("#inputPinPass").val('');
                    $("#id_pinErr").show();
                    $("#id_pinErr").html("6자리 간편번호를 입력해주세요.");
                    $("#inputPinPass").addClass('error');
                    $("#inputPinPassGud").hide();
                    $("#inputPinPassBg").addClass('error');
                    inputPinPassPad.detach(); //가상키패드 디저블 처리
                    return false;
                }
                setTimeout(function(){doPINsignSign($("#id2key").val(), inputPinPassPad.getSecurePIN());}, 200);
            };
            var pinPadOption = {disableClose:false,defaultVisible:_defaultLoginPINpadVisible,closeOnComplete:true,allowGetPIN:true,position:{fillContainer:true}};
            inputPinPassPad.init(targetId, completePINsign_pinpad, pinPadOption);
            inputPinPassPad.setOnLengthChanged(function(inputLen) {
            });
        }    
        //PIN패드 set
        function setLoginPinpad() {
            //if (!window.inputPinPassPad) {
                setLoginPinpadInit('#inputPinPass');
            //} else {
                //inputPinPassPad.attach();
            //}
        }
        function doPINsignSign(id2key, securePIN) {
            inputPinPassPad.detach(); //가상키패드 디저블 처리
            window._time_start_sign = new Date(); //debug
            var tbsData = "PINsign_Login";
            PINsign.login(id2key, securePIN, tbsData, completePINsignSign); //자동개인 및 서명
            $("#inputPinPass").attr("readonly",true);
        }
        
        //베리 전자서명
        function completePINsignSign(result) {
            //alert(JSON.stringify(result));
            $("#inputPinPass").attr("readonly",true);
            window._result_pinsign_sign = result; //debug
            if (window._time_start_sign) window._time_exec_sign = Math.floor((new Date()).getTime()-window._time_start_sign.getTime())/1000; //debug
            if(result.error) {
                var errCode = result.error.code;
                if (errCode == 1) return; //cancel
                if (errCode == 2) {
                    fn_issue();
                    return; //issue
                }
                var errMsg = SITE_getPINsignUserMsg("sign", result.error);
                var pinRetryCount    = result.error.pinRetryCount;
                var maxPinRetryCount = result.error.maxPinRetryCount;
                var pinRemainCount   = maxPinRetryCount-pinRetryCount;
                //console.log('pinRetryCount:'+pinRetryCount+' maxPinRetryCount:'+maxPinRetryCount+' errCode:'+errCode+' errMsg:'+errMsg);
                
                
                //간편번호 오류횟수 체크
                if (errCode == 17015) {
                    $("#id_pinErr").show();
                    $("#id_pinErr").html('간편번호가 일치하지 않습니다. ('+pinRemainCount+'/'+maxPinRetryCount+')<br>[안내] 5회 오류 시 발급된 모든 간편번호 인증서는 폐기됩니다.');
                    $("#inputPinPass").addClass('error');
                    $("#inputPinPassGud").hide();
                    $("#inputPinPassBg").addClass('error');
                }
                //간편번호 오류횟수 초과로 자동폐기된 경우
                else if (errCode == 17011 || errCode == 1003) {
                    //errMsg += " [안내]브라우저에 저장된 간편번호 인증서는 삭제됩니다.";
                    $("#id_pinErr").show();
                    $("#id_pinErr").html('5회 입력 오류로 로그인 하실 수 없습니다.<br>간편번호를 재등록해 주세요.<br>[안내] 브라우저에 저장된 간편번호 인증서는 삭제됩니다.');
                    $("#inputPinPass").addClass('error');
                    $("#inputPinPassGud").hide();
                    $("#inputPinPassBg").addClass('error');
                    PINsign.remove(result.error.berry.id, completePINsign_remove);
                    //if (confirm(errMsg)) PINsign.remove(result.error.berry.id, completePINsign_remove);
                    //pinsignListInit();//등록하기 화면이 나옴
                    //$("#id_pin_area_input_bottom").attr("href","/cpm/mb/CPMMB0203_01.hc");//간편번호 등록으로 이동
                }                
                //폐기된 간편번호 인증서
                else if (errCode == 17005) {
                    errMsg += " [안내]브라우저에 저장된 간편번호 인증서는 삭제됩니다.";
                    $("#id_pinErr").show();
                    $("#id_pinErr").html(errMsg);
                    $("#inputPinPass").addClass('error');
                    $("#inputPinPassGud").hide();
                    $("#inputPinPassBg").addClass('error');
                    PINsign.remove(result.error.berry.id, completePINsign_remove);
                    //if (confirm(errMsg)) PINsign.remove(result.error.berry.id, completePINsign_remove);
                    pinsignListInit();
                    openAlertType1(errMsg);
                }
                //만료된 간편번호 인증서
                else if (errCode == 22311) {
                    errMsg += " [안내]브라우저에 저장된 간편번호 인증서는 삭제됩니다.";
                    $("#id_pinErr").show();
                    $("#id_pinErr").html('간편번호 등록 후 3개월이 경과되어 <br>간편번호 변경이 필요합니다.');
                    $("#inputPinPass").addClass('error');
                    $("#inputPinPassGud").hide();
                    $("#inputPinPassBg").addClass('error');
                    PINsign.remove(result.error.berry.id, completePINsign_remove);
                    //if (confirm(errMsg)) PINsign.remove(result.error.berry.id, completePINsign_remove);
                    pinsignListInit();
                    openAlertType1(errMsg);
                }
                else {
                    //openAlertType1(errMsg);
					$("#id_pinErr").show();
					$("#id_pinErr").html(errMsg);
                    $("#inputPinPass").addClass('error');
                    $("#inputPinPassGud").hide();
                    $("#inputPinPassBg").addClass('error');
                }
                $("#inputPinPass").attr("readonly",true);
                //$("#inputPinPass").attr("disabled",true);
                return;
            }
            
            $("#inputPinPass").attr("readonly",true);
            $("#processType").val("login");
            $("#jws").val(result.data.signedData);
            $("#vidRandom").val(result.data.vidRandom);
            $("#svidRandom").val(result.data.svidRandom);
            $("#id2key").val(result.data.id2key);
            $("#execTime").val(_time_exec_sign);
            
            var url="/cpm/mb/apiCPMMB0203_05.hc";
            $.hcAjax({
                showLoading : true,
                category:"data",
                type:"post",
                url:url,
                data:$("#signForm").serialize(),
                success:function(data) {
                    var rsltCode = data.rsltCode;
                    var rsltMsg  = data.rsltMsg;
                    if(rsltCode == '000'){
                        //console.log('사용자정보가 조회되었습니다.');
                        $("#id_pinErr").html('');
                        $("#id_pinErr").hide();
                        $("#inputPinPass").removeClass('error');
                        $("#inputPinPassGud").show();
                        $("#inputPinPassBg").removeClass('error');
                        
                        HDC.showLoading();
                        $("#loginGb").val("pinSign");
                        $('#form1').attr({'action': '/cpm/mb/CPMMB0101_02.hc', 'reCAPTCHA': ''}).submit();
                    }else{
                        //openAlertType1(rsltMsg);
                        $("#id_pinErr").show();
                        $("#id_pinErr").html(rsltMsg);
                        $("#inputPinPass").addClass('error');
                        $("#inputPinPassGud").hide();
                        $("#inputPinPassBg").addClass('error');
                    }                    
                    return;
                },
                error:function(code, message, data, textStatus, xhr) {
                    //openAlertType1('회원정보 조회 중 오류가 발생하였습니다.');
                    $("#id_pinErr").show();
                    $("#id_pinErr").html('회원정보 조회 중 오류가 발생하였습니다.');
                    $("#inputPinPass").addClass('error');
                    $("#inputPinPassGud").hide();
                    $("#inputPinPassBg").addClass('error');
                    return;
                }
            });            
        }
        
        function completePINsign_remove(result) {
            window._result_pinsign_remove = result; //debug
            if(result.error) {
                var errCode = result.error.code;
                var errMsg = SITE_getPINsignUserMsg("remove", result.error);
                //openAlertType1(errMsg);
                $("#id_pinErr").show();
                $("#id_pinErr").html(errMsg);
                $("#inputPinPass").addClass('error');
                $("#inputPinPassGud").hide();
                $("#inputPinPassBg").addClass('error');
                return;
            }
        }

        
        
        function APPCARD_LOGIN_AjaxCall(){ 
            var msg = null;
            var setTime = 180; //기본 초 
            npPfsCtrl.waitSubmit(function(){       
				var params = npPfsCtrl.serializeArray(deviceForm,false);
                $.hcAjax({
                    showLoading : true,
                    category:"data",
                    url:"/cpm/mb/apiAPPCARDLOGIN.hc",
                    data : params,
                    contentType : 'application/x-www-form-urlencoded; charset=UTF-8',
                    type : 'post',
                    success:function(data){   
                        HDC.hideLoading();
                        var randomNum = data.randomNum;
                        var qrcode = "<img src='/common/QRCODE.hc?p="+randomNum +"' alt='로그인 QR코드'>";
                                   
                        $("#QRCODE").html(qrcode);      
						$("#orTxt").html("또는"); 
                        $("#randomNum").text(randomNum);
						
						//로그인 가능여부 체크
						setTimeout(function(){hcdAppLoginCheck();}, 3000);                       
						
                        tid = setInterval(function(){
                            //console.log("1 loginTab2 visible:"+$('#loginTab2').is(':visible'));
                            if($('#loginTab2').is(':visible') == false){
                                clearInterval(tid);
                                clearInterval(qrcheckid);
                            }
                            else{
                                var varSecond = setTime%60;
                                if(varSecond < 10){
                                    varSecond = '0' + varSecond;
                                }
                                
                                msg = "남은 시간 " + Math.floor(setTime / 60) + ":" + varSecond + "";
                                $("#ViewTimer").html(msg);
                                
                                setTime--;
                                
                                if(setTime<0) { //시간종료
                                    clearInterval(tid);
                                    clearInterval(qrcheckid);//서버체크종료
                                    $(".refresh").show();
                                    $(".restart").hide();
                                }                                
                            }                         
                            
                        }, 1000);            
                    }, 
                    error:function(code, message, data){ 

                    }
                }); 
            }); 
        }
    
		 
		if( nCount == 0 && isAppCardYn == 'Y'){
			try{
				npPfsStartupV2(document.deviceForm, [false, false, false, false, false, true], null, null);
				clearInterval(tid);        
				APPCARD_LOGIN_AjaxCall(); 
			}catch(e){
			}
			nCount++;
		}

		
		var hcdAppLogin = function(){
		    $(".refresh").hide();
		    $(".restart").show();
			$("#QRCODE").html("");      
			$("#orTxt").html(""); 
			$("#randomNum").text("");
			$("#ViewTimer").html("");
			try{
				npPfsStartupV2(document.deviceForm, [false, false, false, false, false, true], null, null);
				clearInterval(tid);
				clearInterval(qrcheckid);
				APPCARD_LOGIN_AjaxCall(); 
			}catch(e){
			}
		}
		
		
		var hcdAppLoginCheck = function(){
			qrcheckid = setInterval(function(){
			    //console.log("2 loginTab2 visible:"+$('#loginTab2').is(':visible'));
                if($('#loginTab2').is(':visible') == false){
                    clearInterval(tid);
                    clearInterval(qrcheckid);
                }else{
                    $.hcAjax({
                        showLoading : false,
                        category:"data",
                        url:"/cpm/mb/apiCPMMB0101_04.hc",
                        data : {},
                        type : 'post',
                        success:function(data){   
                            var sessionInfo = data.result.sessionInfo;
                            //console.log(sessionInfo+", "+qrcheckid);
                            if(sessionInfo==null || sessionInfo==undefined){
                                openAlertType1('서버통신간 오류가 발생하였습니다. 잠시 후 다시시도하세요');
                                clearInterval(qrcheckid);
                            }
                            if(sessionInfo=="qrok"){
                                clearInterval(qrcheckid);
                                HDC.showLoading();
                                $("#loginGb").val("appCard");
                                $('#form1').attr({'action': '/cpm/mb/CPMMB0101_02.hc', 'reCAPTCHA': ''}).submit();
                            }        
                        }, 
                        error:function(code, message, data){
                        }
                    });                    
                }			
				
			}, 3000); 
		}			

		
        var fixFooter = {
                init: function() {
                    var that = this;
                    var fixHeaderHeight = $(header).height();
                    var footer = $('.main_footer');
                    var footerPos = footer.offset().top - 120; //marginTop value
                    var bottom = footer.outerHeight();
                    
                    $(window).on('scroll', function(){
                        var sT = $(window).scrollTop();
                        var iH = $('.main_container .left_col').innerHeight();
                        var sH = $('body').prop('scrollHeight');
                        
                        
                        if(sT >= footerPos) { //시작점
                            if(sH > iH ) {
                                footer.addClass('scroll-able').css('top', sT - 120);
                            }
                        } else {
                            footer.removeClass('scroll-able').removeAttr('style');
                        }
                    });
                }
            };


    </script>
 
 
 
        <!-- //Contents  -->
    </div>
