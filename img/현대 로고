<script type="text/javascript">
    var console = window.console || {log:function(){}};
    var strPsnlWebMenuCd  = '';
    var strScreUrl        = '';
    var strWebPrgmNm      = '';
    var menuDepth         = 0;
    var varPsnlWebMenuCd2 = "";
    var varPsnlWebMenuCd4 = "";
    var varPsnlWebMenuCd6 = "";
    var subMenuHtml       = "";
    var midPath           = window.location.pathname;
    var isUrlInclude      = false;
    var requestUri        = 'index';
    var requestObj        = hcPcPage[requestUri];
    
    if(requestObj == null || requestObj == undefined){
        if(midPath=="/index.jsp" || "/"){
            subMenuHtml = "홈";
        }
    }
    else{
        strPsnlWebMenuCd  = $.trim(requestObj.psnlWebMenuCd);
        strScreUrl        = $.trim(requestObj.screUrl);
        strWebPrgmNm      = $.trim(requestObj.webPrgmNm);
    
        var varScreUrl       = strScreUrl;      //hcPcMenu.screUrl;
        var varPsnlWebMenuCd = strPsnlWebMenuCd;//hcPcMenu.psnlWebMenuCd;
        var varMenuNm        = strWebPrgmNm;    //hcPcMenu.menuNm;
        menuDepth = varPsnlWebMenuCd.length/2;
        if(menuDepth == 1){
            varPsnlWebMenuCd2 = varPsnlWebMenuCd;
            varPsnlWebMenuCd4 = "";
            varPsnlWebMenuCd6 = "";
        }
        else if(menuDepth == 2){
            varPsnlWebMenuCd2 = varPsnlWebMenuCd.substr(0,2);
            varPsnlWebMenuCd4 = varPsnlWebMenuCd.substr(0,4);
            varPsnlWebMenuCd6 = "";
        }
        else if(menuDepth == 3){
            varPsnlWebMenuCd2 = varPsnlWebMenuCd.substr(0,2);
            varPsnlWebMenuCd4 = varPsnlWebMenuCd.substr(0,4);
            varPsnlWebMenuCd6 = varPsnlWebMenuCd.substr(0,6);
        }//if
        
        //subMenuHtml = "홈";
        for (var i =0 ; i < hcPcMenuArr.length; ++i){
            if(hcPcMenuArr[i].psnlWebMenuCd == varPsnlWebMenuCd2){
                if(hcPcMenuArr[i].screUrl.indexOf(strScreUrl) != -1){
                    isUrlInclude = true;
                }
                subMenuHtml += hcPcMenuArr[i].menuNm
            }
            else if(hcPcMenuArr[i].psnlWebMenuCd == varPsnlWebMenuCd4){   
                if(hcPcMenuArr[i].screUrl.indexOf(strScreUrl) != -1){
                    isUrlInclude = true;
                }   
                subMenuHtml += ":"+hcPcMenuArr[i].menuNm;
            }
            else if(hcPcMenuArr[i].psnlWebMenuCd == varPsnlWebMenuCd6){ 
                if(hcPcMenuArr[i].screUrl.indexOf(strScreUrl) != -1){
                    isUrlInclude = true;
                }
                subMenuHtml += ":"+hcPcMenuArr[i].menuNm;
            }//if
        }//for
    }//if
    
    if(!isUrlInclude && strWebPrgmNm.length > 0 ){
        subMenuHtml += ":"+strWebPrgmNm;
    }
    var headerTitle = $('html').find('title').text().split("|")[0];
    headerTitle = headerTitle.replace(/(\s*)/g,'');
    if(subMenuHtml==""){
	    INFO.vst_page_nm = headerTitle;
	}else{
	    INFO.vst_page_nm = subMenuHtml;
	}
    
    //console.log(INFO);     
</script>

