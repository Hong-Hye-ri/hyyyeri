<script type="text/javascript" src="/docfiles/solution/search/ark_new.js?t=163000"></script>
<script type="text/javascript" src="/docfiles/solution/search/search.js?t=163000" charset="UTF-8"></script>    
<script type="text/javascript">
var cookieSearchRecentWord = "HC_SRCH_RCNT_WORD";
/******************************************************************************
 * 인기검색어
 ******************************************************************************/

// 인기검색어 표시 (최대 10건 -> 7건)
function showSearchPopularWord() {    
    try {
         $.getJSON("/solution/searchApi/inc/recommend.jsp",{query:"getPopular"}, function(data){
            // make html
            var html = '';
            
            var array = data.Data.Word;
            var arrayLenth = array.length;
			if(arrayLenth > 7 ){
				arrayLenth = 7;
			}
            if (array != null && arrayLenth > 0) {
                if (data.Data.TotalCount  == 1) {
                    html += '<a href="javascript:void(0)" onclick="selectSearchWord(\'' + array + '\')">' + array + '</a>';
                }else {
                    for (var i = 0; i < arrayLenth; i++) {
                        var field = array[i];
                        html += '<a href="javascript:void(0)" onclick="selectSearchWord(\'' + field + '\')">' + field + '</a>';
                    }
                }
            }
            
            // display
            $(".recent_popular_keyword").html(html);
        }).fail(function(data){ // 데이타 없을 경우 처리
            //alert("검색결과 조회중 오류가 발생했습니다.");
        });
    } catch(ex) {
        //console.log(ex);
    }
}

/******************************************************************************
 * 최근검색어
 ******************************************************************************/
function showSearchRecentWord() {// 최근검색어 표시 (최대 10건)
    //console.log("cookieSearchRecentWord:"+cookieSearchRecentWord);
    var myKeyword = getCookie(cookieSearchRecentWord);
    //console.log("myKeyword:"+myKeyword);
    
    if( myKeyword== null || typeof myKeyword == 'undefined') {
        $(".search_msg").show();
        $(".keyword_list").empty();
        $(".recent_keyword_list").hide();
        $(".recent_tit").hide();
        $("#mid_recent_title").hide();
        return;
    }
    var myKeywords = myKeyword.split("^%");
    showMyKeyword(myKeywords);
}
function showMyKeyword(myKeywords) {
    var obj = $(".recent_keyword_list");
    obj.html('');
    
    var html = '';
    var cnt = 0;
    for( var i = 0; i < myKeywords.length; i++) {
        if( myKeywords[i] == "") continue;
        html +=  '<li>';
        html +=  '<a href="javascript:selectSearchWord(\'' + myKeywords[i] + '\')" class="p1_m_lt_1ln">' + myKeywords[i] + '</a>'; 
        html +=  '<button type="button" onClick="removeMyKeyword(\'' + myKeywords[i] + '\');" class="p2_mrt_1ln fc_m_link btn_kw_del">삭제</button>';
        html +=  '</li>';
        cnt++;
    }
    obj.html(html);
    if (cnt == 0){
        obj.html("");
        $(obj).hide();
        $(".search_msg").show();
        $(".keyword_list").empty();
		$(".recent_keyword_list").hide();
        $(".recent_tit").hide();
        $("#mid_recent_title").hide();
    }
    else{
        $(".search_msg").hide();
		$(".recent_keyword_list").show();
        $(".recent_tit").show();
        $("#mid_recent_title").show();
    }
}
//내가 찾은 검색어 삭제
function removeMyKeyword(keyword) {
    var myKeyword = getCookie(cookieSearchRecentWord);
    if( myKeyword == null) {
        myKeyword = "";
    }
    var myKeywords = myKeyword.split("^%");
    var i = 0;
    while (i < myKeywords.length) {
        if (myKeywords[i] == keyword) {
            myKeywords.splice(i, 1);
        } else { 
            i++; 
        }
    }
    setCookie(cookieSearchRecentWord, myKeywords.join("^%"), 90);
    showMyKeyword(myKeywords);
}
//전체 검색어 삭제
function removeAllSearchWord(){
    var myKeyword = getCookie(cookieSearchRecentWord);
    if( myKeyword == null) {
        myKeyword = "";
    }
    var myKeywords = myKeyword.split("^%");
    var i = 0;
    while (i < myKeywords.length) {
        myKeywords.splice(i, 1);
    }
    setCookie(cookieSearchRecentWord, myKeywords.join("^%"), 90);
    showMyKeyword(myKeywords);
}
                    
//내가 찾은 검색어 조회
function getMyKeyword(keyword) {
    var MYKEYWORD_COUNT = 8; //내가 찾은 검색어 갯수 + 1
    var myKeyword = getCookie(cookieSearchRecentWord);
    if( myKeyword== null) {
        myKeyword = "";
    }
    var myKeywords = myKeyword.split("^%");
    var i = 0;
    var existsKeyword = false;
    for( var i = 0; i < myKeywords.length; i++) {
        if( myKeywords[i] == keyword) {
            existsKeyword = true;
            break;
        }
    }
    if( !existsKeyword ) {
        myKeywords.push(keyword);
        if( myKeywords.length == MYKEYWORD_COUNT) {
            myKeywords = myKeywords.slice(1,MYKEYWORD_COUNT);
        }
    } else {
        while (i < myKeywords.length) {
            if (myKeywords[i] == keyword) {
                myKeywords.splice(i, 1);
            } else { 
                i++; 
            }
        }
        myKeywords.push(keyword);
    }
  
    //console.log(myKeywords.join("^%"));
    setCookie(cookieSearchRecentWord, myKeywords.join("^%"), 90);
    
    showMyKeyword(myKeywords.reverse());
}
//쿠키값 조회
function getCookie(c_name) {
    try {
        var i,x,y,cookies=document.cookie.split(";");
        for (i=0;i<cookies.length;i++) {
            x=cookies[i].substr(0,cookies[i].indexOf("="));
            y=cookies[i].substr(cookies[i].indexOf("=")+1);
            x=x.replace(/^\s+|\s+$/g,"");
            
            if (x==c_name) {
                if (decodeURIComponent) return decodeURIComponent(y);
            }        
        }
    } catch (e) {}
}
//쿠키값 설정
function setCookie(c_name,value,exdays) {
    var exdate=new Date();
    exdate.setDate(exdate.getDate() + exdays);
    var c_value=encodeURIComponent(value) + ((exdays==null) ? "" : "; expires="+exdate.toUTCString());
    document.cookie=c_name + "=" + c_value +'; path=/ ';
}

/******************************************************************************
 * 검색 공통
 ******************************************************************************/
function selectSearchWord(word) { //검색어 선택
    var obj = $("#topSearch");
    obj.val(word);
    
    getMyKeyword(word);
    $("#search_gnb").submit(); //2017-10-13
}

//--------------------------------------------------------------------
function submitSearchGNB() {
    var obj = $('#topSearch');
    var checkVal = obj.val();
    
    if (checkVal == null || checkVal.length == 0) {
        openAlertType1("검색어를 입력해 주세요.","",function(){obj.focus();});
        $('.modal_pop.modal_alert.active').css("z-index","10001");
        //obj.focus();
        return false;
    }
    
    if(typeof Ntm !== "undefined"){
        Ntm.Event.fireUserDefined("searchKeyword", {searchKeyword:checkVal});        
    }
    
    getMyKeyword(checkVal);
    return true;
}    

$(document).ready(function() {
    //// 검색버튼 클릭 후 검색 활성화 테스트 용
    //setTimeout(function(){
    //    if($('#header').find('.btn_search_open').length) {
    //        $('#header').on('click', '.btn_search_open', function(){
    //            $('.section-search').addClass('on').find('input[type=text]').focus();
    //        });
    //    }
    //}, 3000);            
    //// 검색 활성시 input 포커스 이동
    //$('input[type=text]').focus();
    
    //blur 영역을 클릭하면 닫기 기능 수행
    $(".section-search").mouseup(function(e){
        var searchListArea = $('.search_list');
        var searchHeadArea = $('.search_head');
        if(!searchListArea.is(e.target) && searchListArea.has(e.target).length === 0 ){
            if(!searchHeadArea.is(e.target) && searchHeadArea.has(e.target).length === 0 ){
                $('.section-search').removeClass('on');
                $('html').removeClass('layer_active');
            }
        }
    });

    //검색 레이어 닫기
    $('.btn_search_close').on('click', function(){
        $('.section-search').removeClass('on');
        $('html').removeClass('layer_active');
        $('#btnSearchOpen').focus();
    });
    //검색어 삭제
    $('#btnSearchdel').on('click', function(){
        $('#topSearch').val('');
        $('.box_search_input').removeClass('focused on');
    });
});
</script>
